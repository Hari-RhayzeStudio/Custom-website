generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
}

model ProductAsset {
  id                    BigInt  @id(map: "product_assets_duplicate_pkey") @default(autoincrement())
  sku                   String  @unique(map: "product_assets_duplicate_sku_key") @db.VarChar(255)
  status                String  @default("Empty") @db.VarChar(50)
  category              String? @db.VarChar(255)
  pre_image_url         String?
  wax_image_url         String?
  wax_image_alt_text    String? @db.VarChar(255)
  wax_description       String?
  cast_image_url        String?
  cast_image_alt_text   String? @db.VarChar(255)
  cast_description      String?
  final_image_url       String?
  final_image_alt_text  String? @db.VarChar(255)
  final_description     String?
  meta_title            String? @db.VarChar(255)
  meta_description      String? @db.VarChar(500)
  product_name          String? @db.VarChar(255)
  sketch_image_url      String?
  sketch_image_alt_text String? @db.VarChar(255)
  scetch_description    String? @map("sketch_description")
  created_at            String? @default(dbgenerated("to_char((now() AT TIME ZONE 'America/Chicago'::text), 'MM-DD-YYYY HH12:MI AM'::text)")) @db.VarChar(30)
  modified_at           String? @default(dbgenerated("to_char((now() AT TIME ZONE 'America/Chicago'::text), 'MM-DD-YYYY HH12:MI AM'::text)")) @db.VarChar(30)

  @@map("product_assets_staging")
}

