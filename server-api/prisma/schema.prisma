datasource db {
  provider  = "postgresql"
  url       = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
}

generator client {
  provider = "prisma-client-js"
}

model ProductAsset {
  id                     BigInt   @id @default(autoincrement())
  sku                    String   @unique
  product_name           String?
  category               String?
  status                 String?
  
  scetch_description     String?  @map("sketch_description") 
  wax_description        String?
  cast_description       String?
  final_description      String?
  
  final_image_url        String?
  wax_image_url          String?
  sketch_image_url       String?
  cast_image_url         String?
  
  meta_title             String?
  meta_description       String?
  created_at             String?
  modified_at            String?

  @@map("product_assets_staging") 
}